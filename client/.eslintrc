{
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "plugins": [
      "react"
    ],
    "parser": "@typescript-eslint/parser",
    "extends": ["eslint:recommended",
    "plugin:react/recommended"],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "error"
    }
  }